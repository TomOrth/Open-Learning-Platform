<ul class="nav nav-justified nav-tabs" id="justifiedTab" role="tablist">
  <li class="nav-item">
    <a aria-controls="verification" aria-selected="false" class="nav-link" data-toggle="tab" href="#verification" id="verification-tab" role="tab">verification</a>
  </li>
  <li class="nav-item">
    <a aria-controls="plans" aria-selected="true" class="nav-link active" data-toggle="tab" href="#plans" id="plans-tab" role="tab">plans</a>
  </li>
</ul>
<div class="tab-content" id="justifiedTabContent">
  <div aria-labelledby="verification-tab" class="tab-pane fade" id="verification" role="tabpanel">
      <%= render 'partials/educators/verification', educator: @educator %>
  </div>
  <div aria-labelledby="plans-tab" class="tab-pane fade show active" id="plans" role="tabpanel">
    <% if @educator.verified %>
      <%= link_to 'New Lesson Plan', new_lesson_plan_path, class: "btn btn-primary" %> 
      <%= render 'partials/lesson_plans', lesson_plans: @lesson_plans %>
    <% else %>
       <p>Not verified</p>
    <% end %>
  </div>
  <div aria-labelledby="contact-tab" class="tab-pane fade" id="contact" role="tabpanel">...</div>
</div>
